<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.mavenjavafxpokus1.katalognemovitosti.FXMLController">
  <children>
    <VBox prefHeight="200.0" prefWidth="100.0" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <TableView id="table" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="SOMETIMES">
          <columns>
            <TableColumn editable="true" maxWidth="5000.0" minWidth="10.0" prefWidth="100.0" sortType="ASCENDING" sortable="true" text="Typ" fx:id="typTableColumn" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="100.0" text="Plocha" fx:id="plochaTableColumn" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="100.0" text="Místnosti" fx:id="mistnostiTableColumn" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="100.0" text="Cena" fx:id="cenaTableColumn" />
          </columns>
        </TableView>
        <HBox alignment="CENTER_RIGHT" maxHeight="38.0" minHeight="38.0" prefHeight="38.0" prefWidth="-1.0" spacing="5.0" VBox.vgrow="SOMETIMES">
          <children>
            <Button fx:id="odebratButton" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleOdebratButtonAction" prefWidth="160.0" text="Odebrat nemovitost" textAlignment="CENTER" textOverrun="ELLIPSIS" HBox.hgrow="NEVER" />
            <Button fx:id="pridatButton" mnemonicParsing="false" onAction="#handlePridatButtonAction" prefWidth="160.0" text="Přidat nemovitost" />
          </children>
        </HBox>
      </children>
      <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
    </VBox>
  </children>
</AnchorPane>
